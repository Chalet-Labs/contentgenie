{
  "name": "contentgenie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "doppler run -- next dev --turbo",
    "build": "doppler run -- next build",
    "start": "doppler run -- next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "db:generate": "doppler run -- drizzle-kit generate",
    "db:push": "doppler run -- drizzle-kit push",
    "db:studio": "doppler run -- drizzle-kit studio",
    "trigger:dev": "doppler run -- bunx trigger.dev@latest dev",
    "trigger:deploy": "doppler run -- bunx trigger.dev@latest deploy",
    "prepare": "husky",
    "vercel-build": "if [ \"$VERCEL_ENV\" = \"preview\" ]; then npx drizzle-kit push --force; fi && next build"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.37.0",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@trigger.dev/react-hooks": "^4.3.3",
    "@trigger.dev/sdk": "^4.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.563.0",
    "next": "^14.2.25",
    "next-themes": "^0.4.6",
    "rate-limiter-flexible": "^9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rss-parser": "^3.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "drizzle-kit": "^0.31.8",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.3",
    "husky": "^9.1.7",
    "jsdom": "^28.0.0",
    "msw": "^2.12.9",
    "playwright": "^1.58.0",
    "postcss": "^8.4.38",
    "puppeteer": "^24.36.1",
    "storybook": "^8.6.15",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vitest": "^4.0.18"
  }
}
